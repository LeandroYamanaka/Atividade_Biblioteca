@model Usuario

<div class="row justify-content-md-center">
    <form asp-action="CadastroUsuario" method="POST" class="col-md-4">
        <input type="hidden" asp-for="IdUsuario" />
        <div class="form-group">
            <label asp-for="Nome">Nome</label>
            <input asp-for="Nome" class="form-control"/>
        </div>
        <div class="form-group">
            <label asp-for="Telefone">Telefone</label>
            <input asp-for="Telefone" class="form-control"/>
        </div>
        <div>
            <label asp-for="Email">Email</label>
            <input asp-for="Email" class="form-control" />
        </div>
        <div>
            <label asp-for="Nascimento">Nascimento</label>
            <input asp-for="Nascimento" class="form-control" />
        </div>
        <div>
            <label asp-for="Login">Login</label>
            <input asp-for="Login" class="form-control" />
        </div>
        <div>
            <label asp-for="Senha">Senha</label>
            <input asp-for="Senha" class="form-control" />
        </div>
        <div>
            <label asp-for="Nivel">Nível:</label>
            <select name="Nivel" select id="Nivel" size="1">
                <option value="Administrador">Administrador</option>
                <option value="Usuario">Usuário</option>
            </select>
        </div>
        <input type="submit" value="Salvar" class="btn btn-primary"/>
    </form>
</div>